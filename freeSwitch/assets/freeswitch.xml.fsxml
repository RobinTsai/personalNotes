                   <action application="log" data="NOTICE agent_number=${consult_agent_number}" inline="true"/>
                   <action application="set" data="ignore_early_media=true"/>
                   <action application="set" data="instant_ringback=true"/>
                   <action application="set" data="ringback=${consult_hold_music}"/>
                   <action application="set" data="transfer_fallback_destination=xxxxx_transfer_unpark-${consult_agent_number}}"/>
                   <action application="set" data="failure_causes=NORMAL_CLEARING,USER_NOT_REGISTERED,ALLOTTED_TIMEOUT,NORMAL_TEMPORARY_FAILURE,CALL_REJECTED,USER_BUSY,NO_ANSWER,NO_USER_RESPONSE,TIMEOUT,NO_ROUTE_DESTINATION,RECOVERY_ON_TIMER_EXPIRE,NETWORK_OUT_OF_ORDER,SUBSCRIBER_ABSENT"/>
                   <action application="set" data="transfer_on_fail=NORMAL_CLEARING,USER_NOT_REGISTERED,ALLOTTED_TIMEOUT,NORMAL_TEMPORARY_FAILURE,CALL_REJECTED,USER_BUSY,NO_ANSWER,NO_USER_RESPONSE,TIMEOUT,NO_ROUTE_DESTINATION,RECOVERY_ON_TIMER_EXPIRE,NETWORK_OUT_OF_ORDER,SUBSCRIBER_ABSENT xxxxx_transfer_unpark-$1@$2"/>
                   <action application="set" data="transfer_after_bridge=in_ivr_park:XML:xxxxx_cti"/>
               </condition>
               <condition field="${consult_hold_music}" expression="^Undefined$" break="never">
                   <action application="set" data="ignore_early_media=false"/>
                   <action application="set" data="instant_ringback=false"/>
               </condition>
               <condition field="${consult_agent_number}" expression="^([1-9]\d{13})$" break="on-true">
                   <action application="bridge" data="user/${consult_agent_number}"/>
               </condition>
               <condition field="${consult_agent_number}" expression="^(\d+)$" >
                   <action application="set" data="effective_caller_id_name=${lin_called_display}"/>
                   <action application="set" data="effective_caller_id_number=${lin_called_display}"/>
               </condition>
               <condition field="${consult_agent_number}" expression="^(\d+)$" require-nested="false" break="never">
                   <condition field="${specified_caller_to_callee}" expression="^(\d+)-to-${consult_agent_number}$" break="never">
                       <action application="set" data="effective_caller_id_name=${1}"/>
                       <action application="set" data="effective_caller_id_number=${1}"/>
                   </condition>
               </condition>
               <condition field="${consult_agent_number}" expression="^(\d+)$" >
                   <action application="export" data="absolute_codec_string=PCMA,PCMU"/>
                   <action application="bridge" data="sofia/gateway/${expand(distributor xxxxx_gateways ${sofia(profile external gwlist down)})}/${consult_agent_number}"/>
               </condition>
           </extension>
           <extension name="xxxxx_transfer_unpark">
               <condition field="destination_number" expression="^xxxxx_transfer_unpark-(.*)$">
                   <action application="unset" data="ringback"/>
                   <action application="unset" data="transfer_fallback_destination"/>
                   <action application="unset" data="transfer_after_bridge"/>
                   <action application="unset" data="transfer_on_fail"/>
                   <action application="unset" data="ringback"/>
                   <action application="bridge_export" data="nolocal:park_after_bridge=true"/>
                   <action application="answer"/>
                   <action application="fifo" data="$1 out nowait"/>
               </condition>
           </extension>
       </context>
