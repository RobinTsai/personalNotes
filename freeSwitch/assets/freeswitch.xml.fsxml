           <extension name="xxxxx_consult_agent">
               <condition field="destination_number" expression="^xxxxx_consult_agent-(.*)$" >
                   <action application="log" data="NOTICE agent_number=${consult_agent_number}" inline="true"/>
                   <action application="sleep" data="100"/>
                   <action application="set" data="consult_fallback_destination=xxxxx_consult_unpark-${consult_agent_number}}"/>
                   <action application="set" data="transfer_after_bridge=xxxxx_consult_unpark-$1"/>
                   <action application="set" data="failure_causes=NORMAL_CLEARING,USER_NOT_REGISTERED,ALLOTTED_TIMEOUT,NORMAL_TEMPORARY_FAILURE,CALL_REJECTED,USER_BUSY,NO_ANSWER,NO_USER_RESPONSE,TIMEOUT,NO_ROUTE_DESTINATION,RECOVERY_ON_TIMER_EXPIRE,NETWORK_OUT_OF_ORDER,SUBSCRIBER_ABSENT"/>
                   <action application="set" data="transfer_on_fail=NORMAL_CLEARING,USER_NOT_REGISTERED,ALLOTTED_TIMEOUT,NORMAL_TEMPORARY_FAILURE,CALL_REJECTED,USER_BUSY,NO_ANSWER,NO_USER_RESPONSE,TIMEOUT,NO_ROUTE_DESTINATION,RECOVERY_ON_TIMER_EXPIRE,NETWORK_OUT_OF_ORDER,SUBSCRIBER_ABSENT xxxxx_consult_unpark-$1"/>
