                   <action application="set" data="transfer_on_fail=NORMAL_CLEARING,USER_NOT_REGISTERED,ALLOTTED_TIMEOUT,NORMAL_TEMPORARY_FAILURE,CALL_REJECTED,USER_BUSY,NO_ANSWER,NO_USER_RESPONSE,TIMEOUT,NO_ROUTE_DESTINATION,RECOVERY_ON_TIMER_EXPIRE,NETWORK_OUT_OF_ORDER,SUBSCRIBER_ABSENT xxxxx_consult_unpark-$1"/>
