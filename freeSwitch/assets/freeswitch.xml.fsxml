       </configuration>
       <configuration name="translate.conf" description="Number Translation Rules">
           <profiles>
               <profile name="US">
                   <rule regex="^\+(\d+)$" replace="$1"/>
                   <rule regex="^(1[2-9]\d{2}[2-9]\d{6})$" replace="$1"/>
                   <rule regex="^([2-9]\d{2}[2-9]\d{6})$" replace="1$1"/>
                   <rule regex="^([2-9]\d{6})$" replace="1${areacode}$1"/>
                   <rule regex="^011(\d+)$" replace="$1"/>
               </profile>
               <profile name="GB">
                   <rule regex="^\+(\d+)$" replace="$1"/>
                   <rule regex="^$" replace="$1"/>
               </profile>
               <profile name="HK">
                   <rule regex="\+(\d+)$" replace="$1"/>
                   <rule regex="^(852\d{8})$" replace="$1"/>
                   <rule regex="^(\d{8})$" replace="852$1"/>
               </profile>
           </profiles>
       </configuration>
       <configuration name="tts_commandline.conf" description="TextToSpeech Commandline configuration">
           <settings>
               <param name="command" value="ontts -t ${text} -o ${file} -lp 'appid = 587862ee, work_dir = .'"/>
           </settings>
       </configuration>
       <configuration name="xxxxx_play.conf" description="xxxxx Play Configuration">
           <settings>
               <param name="proxy_url_file" value="http://192.168.1.112:8089/file"/>
               <param name="proxy_url_tts" value="http://192.168.1.112:8089/tts"/>
               <param name="file_path_prefix" value="/var/cc_soundfile/"/>
           </settings>
       </configuration>
       <configuration name="xml_curl.conf" description="cURL XML Gateway">
           <bindings>
               <binding name="directory">
                   <param name="gateway-url" value="127.0.0.1:4002/agent/generatexml?timestamp=20170107230000" bindings="directory"/>
               </binding>
           </bindings>
       </configuration>
       <configuration name="zeroconf.conf" description="Zeroconf Event Handler">
           <settings>
               <param name="publish" value="yes"/>
               <param name="browse" value="_sip._udp"/>
           </settings>
       </configuration>
   </section>
   <section name="dialplan" description="Regex/XML Dialplan">
       <context name="default">
           <extension name="ivr_test">
               <condition field="destination_number" expression="(^0\d{5}$)">
                   <action application="curl" data="127.0.0.1:4011/apps/stream_tts?AppId='${lin_app_id}'"/>
                   <action application="set" data="stream_tts=${curl_response_data}"/>
                   <action application="log" data="stream tts switch is ${curl_response_data}"/>
                   <action application="bridge_export" data="lin_uuid=${uuid}"/>
                   <action application="bridge_export" data="lin_workflow=ivr_test"/>
                   <action application="bridge_export" data="lin_called_number=${destination_number}"/>
                   <action application="bridge_export" data="lin_caller_number=${caller_id_number}"/>
                   <action application="bridge_export" data="lin_called_display=${destination_number}"/>
                   <action application="bridge_export" data="hold_music=/var/cc_soundfile/queue_hold_music/waitmusic.wav"/>
                   <action application="bridge_export" data="stream_tts=${stream_tts}"/>
                   <action application="set" data="cc_export_vars=lin_uuid"/>
                   <action application="set" data="max_forwards=120"/>
                   <action application="answer"/>
                   <action application="sleep" data="500"/>
                   <action application="transfer" data="in_ivr_park XML xxxxx_cti"/>
               </condition>
           </extension>
           <extension name="to_pstn">
               <condition field="destination_number" expression="(^\d+$)" />
               <condition field="${lin_uuid}" expression="^$" break="never">
                   <action application="export" data="lin_uuid=${uuid}"/>
               </condition>
               <condition field="${lin_workflow}" expression="^out_callback$|^out_direct$">
                   <action application="curl" data="127.0.0.1:4011/apps/stream_tts?AppId='${lin_app_id}'"/>
                   <action application="set" data="stream_tts=${curl_response_data}"/>
                   <action application="log" data="stream tts switch is ${curl_response_data}"/>
                   <action application="export" data="lin_app_id=${lin_app_id}"/>
                   <action application="export" data="lin_workflow=${lin_workflow}"/>
                   <action application="bridge_export" data="stream_tts=${stream_tts}"/>
                   <action application="lua" data="callout_limit.lua ${lin_app_id}"/>
                   <action application="export" data="ignore_early_media=false"/>
                   <action application="set" data="hangup_auto=true"/>
                   <action application="export" data="nolocal:hangup_auto=false"/>
                   <action application="export" data="transfer_after_bridge=in_ivr_park:XML:xxxxx_cti"/>
                   <action application="export" data="early_media_record_file=/var/cc_ringings/${lin_app_id}/${strftime(%Y%m%d%H%M%S)}_${lin_uuid}_${uuid}.wav"/>
                   <action application="export" data="nolocal:RECORD_READ_ONLY=true"/>
                   <action application="mkdir" data="/var/cc_ringings/${lin_app_id}"/>
                   <action application="export" data="nolocal:execute_on_pre_answer_1=record_session::${early_media_record_file} +60"/>
                   <action application="export" data="nolocal:execute_on_pre_answer_2=set lin_user_pre_answer=true"/>
                   <action application="export" data="nolocal:execute_on_answer_1=set lin_user_answered=true"/>
                   <action application="export" data="nolocal:execute_on_answer_2=stop_record_session::${early_media_record_file}"/>
               </condition>
               <condition field="${lin_workflow}" expression="out_callback" break="never">
                   <action application="export" data="lin_uuid=${lin_uuid}"/>
                   <action application="export" data="lin_called_display=${lin_called_display}"/>
                   <action application="export" data="lin_caller_number=${lin_caller_number}"/>
                   <action application="export" data="lin_called_number=${lin_called_number}"/>
                   <action application="export" data="effective_caller_id_name=${lin_called_display}"/>
                   <action application="export" data="effective_caller_id_number=${lin_called_display}"/>
                   <action application="export" data="nolocal:bridge_answer_timeout=${bridge_answer_timeout}"/>
                   <action application="bridge" data="sofia/gateway/${distributor(xxxxx_gateways ${sofia(profile external gwlist down)})}/${destination_number}"/>
               </condition>
               <condition field="${lin_workflow}" expression="out_direct">
                   <action application="export" data="lin_uuid=${uuid}"/>
                   <action application="export" data="lin_called_display=${callout_display_number}"/>
                   <action application="export" data="lin_caller_number=${caller_id_number}"/>
                   <action application="export" data="lin_called_number=${destination_number}"/>
                   <action application="set" data="effective_caller_id_name=${callout_display_number}" />
                   <action application="set" data="effective_caller_id_number=${callout_display_number}" />
                   <action application="set" data="park_timeout=180"/>
                   <action application="park"/>
               </condition>
           </extension>
           <extension name="call_out_direct">
               <condition field="destination_number" expression="call_out_direct">
                   <action application="export" data="effective_caller_id_name=${lin_called_display}"/>
                   <action application="export" data="effective_caller_id_number=${lin_called_display}"/>
                   <action application="export" data="nolocal:bridge_answer_timeout=${bridge_answer_timeout}"/>
                   <action application="bridge" data="sofia/gateway/${distributor(xxxxx_gateways ${sofia(profile external gwlist down)})}/${lin_called_number}"/>
               </condition>
           </extension>
           <extension name="call_inner_call">
               <condition field="destination_number" expression="call_inner_call" />
               <condition field="${lin_caller_number}" expression="^\d*(\d{4})$">
                   <action application="set" data="lin_workflow=inner_call"/>
                   <action application="unset" data="transfer_after_bridge"/>
                   <action application="set" data="effective_caller_id_name=$1"/>
                   <action application="set" data="effective_caller_id_number=$1"/>
                   <action application="bridge" data="user/${lin_called_number}"/>
               </condition>
           </extension>
           <extension name="warnning_play">
               <condition field="destination_number" expression="warnning_play">
                   <action application="playback" data ="tone_stream://%(700,700,450,450);loops=3"  />
                   <action application="hangup"/>
               </condition>
           </extension>
           <extension name="warnning_arrearage">
               <condition field="destination_number" expression="warnning_arrearage">
                   <action application="loop_playback" data ="+3 /var/cc_soundfile/queue_hold_music/${lin_warnning_file}"  />
                   <action application="hangup"/>
               </condition>
           </extension>
           <extension name="ads_call_cti">
               <condition field="destination_number" expression="ads_call_cti">
                   <action application="export" data="lin_uuid=${lin_uuid}"/>
                   <action application="set" data="sip_h_X-call_id=${lin_uuid}"/>
                   <action application="set" data="sip_h_X-ad_version=${ad_version}"/>
                   <action application="set" data="sip_h_X-ad_task_id=${ad_task_id}"/>
                   <action application="set" data="sip_h_X-ad_task_name=${ad_task_name}"/>
                   <action application="set" data="sip_h_X-ad_call_id=${ad_call_id}"/>
                   <action application="set" data="sip_h_X-ad_queue_id=${ad_queue_id}"/>
                   <action application="set" data="sip_h_X-ad_ivr_id=${ad_ivr_id}"/>
                   <action application="set" data="sip_h_X-ad_robot_token=${ad_robot_token}"/>
                   <action application="set" data="sip_h_X-ad_custom_vars=${ad_custom_vars}"/>
                   <action application="set" data="sip_h_X-ad_type=${ad_type}"/>
                   <action application="bridge" data="sofia/gateway/${cti_gateway_name}/ads-${destination_number}"/>
               </condition>
           </extension>
       </context>
       <context name="public">
           <extension name="To_VOIP">
               <condition field="network_addr" expression="^116.62.225.16$|^59.110.105.224$|^139.196.137.44$|^10.71.47.196$|^vos3.xxxxx.cn$|^10.28.207.149$|^59.110.105.200$|^10.24.203.165$|^60.205.94.67$|^192.168.1.88$|^47.94.221.132$|^192.168.1.89$|^47.95.192.214$|^192.168.1.90$|^60.205.205.218$|^192.168.1.102$|^39.96.196.131$|^192.168.1.104$|^39.96.196.164$|^192.168.1.103$|^39.96.190.104$|^192.168.1.109$|^39.96.59.136$|^192.168.1.111$|^39.97.98.130$|^192.168.1.110$|^39.96.85.232$|^192.168.1.112$|^39.0.0.165$|^192.168.1.96$|^123.56.218.144$|^192.168.1.97$|^47.94.201.3$|^192.168.1.98$|^59.110.159.185$|^192.168.1.100$|^47.94.156.144$|^192.168.1.99$|^47.95.249.206$|^192.168.1.91$|^47.93.97.2$|^192.168.1.92$|^47.93.18.231$|^192.168.1.93$|^47.94.248.58$|^192.168.1.95$|^47.94.241.136$|^192.168.1.94$|^47.94.219.57$|^192.168.1.120$|^39.97.111.39$|^10.1.17.83$|^123.60.105.163$"/>
               <condition field="destination_number" expression="^ads-(.*)" break="never">
                   <action application="bridge_export" data="lin_workflow=out_auto"/>
                   <action application="bridge_export" data="destination_number=$1"/>
                   <action application="bridge_export" data="lin_uuid=${sip_h_X-call_id}"/>
                   <action application="bridge_export" data="ad_task_id=${sip_h_X-ad_task_id}"/>
                   <action application="bridge_export" data="ad_task_name=${sip_h_X-ad_task_name}"/>
                   <action application="bridge_export" data="ad_call_id=${sip_h_X-ad_call_id}"/>
                   <action application="bridge_export" data="ad_queue_id=${sip_h_X-ad_queue_id}"/>
                   <action application="bridge_export" data="ad_ivr_id=${sip_h_X-ad_ivr_id}"/>
                   <action application="bridge_export" data="ad_robot_token=${sip_h_X-ad_robot_token}"/>
                   <action application="unset" data="sip_h_X-call_id"/>
                   <action application="unset" data="sip_h_X-ad_task_id"/>
                   <action application="unset" data="sip_h_X-ad_task_name"/>
                   <action application="unset" data="sip_h_X-ad_call_id"/>
                   <action application="unset" data="sip_h_X-ad_queue_id"/>
                   <action application="unset" data="sip_h_X-ad_ivr_id"/>
                   <action application="unset" data="sip_h_X-ad_robot_token"/>
                   <action application="unset" data="sip_h_X-ad_custom_vars"/>
                   <action application="unset" data="sip_h_X-ad_type"/>
                   <anti-action application="bridge_export" data="lin_uuid=${uuid}"/>
                   <anti-action application="bridge_export" data="lin_workflow=in"/>
               </condition>
               <condition field="destination_number" expression="^(.*)">
                   <action application="curl" data="127.0.0.1:4011/apps/appid_by_number?number='${destination_number}'"/>
                   <action application="set" data="lin_app_id=${curl_response_data}"/>
                   <action application="log" data="get appid from number,appid:${curl_response_data}"/>
                   <action application="curl" data="127.0.0.1:4011/apps/stream_tts?AppId='${lin_app_id}'"/>
                   <action application="set" data="stream_tts=${curl_response_data}"/>
                   <action application="bridge_export" data="stream_tts=${stream_tts}"/>
                   <action application="log" data="call in stream tts switch is ${curl_response_data}"/>
                   <action application="set" data="limit_ignore_transfer=true"/>
                   <action application="lua" data="inlimit.lua"/>
                   <action application="bridge_export" data="lin_called_number=${destination_number}"/>
                   <action application="bridge_export" data="lin_caller_number=${caller_id_number}"/>
                   <action application="bridge_export" data="lin_called_display=${destination_number}"/>
                   <action application="bridge_export" data="hold_music=/var/cc_soundfile/queue_hold_music/waitmusic.wav"/>
                   <action application="set" data="vdh_user_id=${sip_h_X-vdh_user_id}"/>
                   <action application="set" data="cc_export_vars=lin_uuid"/>
                   <action application="set" data="max_forwards=120"/>
                   <action application="ring_ready" data="300"/>
                   <action application="answer"/>
                   <action application="sleep" data="500"/>
                   <action application="transfer" data="in_ivr_park XML xxxxx_cti"/>
               </condition>
           </extension>
       </context>
       <context name="xxxxx_cti">
           <extension name="in_ivr_park">
               <condition field="destination_number" expression="in_ivr_park" />
               <condition field="${stream_tts}" expression="^$" break="never">
                   <action application="curl" data="127.0.0.1:4011/apps/stream_tts?AppId='${lin_app_id}'"/>
                   <action application="set" data="stream_tts=${curl_response_data}"/>
                   <action application="bridge_export" data="stream_tts=${stream_tts}"/>
               </condition>
               <condition field="${hangup_auto}" expression="true" >
                   <anti-action application="set" data="park_timeout=360"/>
                   <anti-action application="park"/>
                   <anti-action application="log" data="warning ivr_park exceed 360s"/>
                   <action application="set" data="park_timeout=1" />
                   <action application="park" />
               </condition>
           </extension>
           <extension name="auto_ivr_park">
               <condition field="destination_number" expression="auto_ivr_park">
                   <action application="set" data="park_timeout=${ad_ringing_timeout}:MEDIA_TIMEOUT"/>
                   <action application="park"/>
                   <action application="log" data="warning my_ivr_park timeout"/>
               </condition>
           </extension>
           <extension name="in_ivr_play">
               <condition field="destination_number" expression="in_ivr_play"/>
               <condition field="${play_type}-${stream_tts}" expression="tts-0" break="never">
                   <action application="curl" data="127.0.0.1:4009/generate?tts_content='${tts_content}'&file_path='${file_path}'&tts_config='${tts_config}'"/>
                   <action application="log" data="CURL_RESPONSE tts_response=${curl_response_data}"/>
               </condition>
               <condition field="destination_number" expression="in_ivr_play">
                   <action application="lua" data="ivr_play.lua"/>
                   <action application="park"/>
               </condition>
           </extension>
           <extension name="gather_digits">
               <condition field="destination_number" expression="gather_digits"/>
               <condition field="${tts_file_exists}-${stream_tts}" expression="false-0" break="never">
                   <action application="curl" data="127.0.0.1:4009/generate?tts_content='${tts_content}'&file_path='${tts_file_path}'&tts_config='${tts_config}'"/>
                   <action application="log" data="TTS_CURL_RESPONSE tts_response=${curl_response_data}"/>
               </condition>
               <condition field="destination_number" expression="gather_digits">
                   <action application="lua" data="gather_digits.lua ${gather_min_len} ${gather_max_len} ${gather_timeout} '${tts_file_path}' '${gather_endKey}'"/>
                   <action application="event" data="Event-Subclass=xxxxx::gather_finished,Event-Name=CUSTOM"/>
                   <action application="park"/>
                   <action application="park"/>
               </condition>
           </extension>
           <extension name="ivr_extern">
               <condition field="destination_number" expression="ivr_extern"/>
               <condition field="${tts_file_exists}" expression="false" break="on-true">
                   <action application="curl" data="127.0.0.1:4009/generate?tts_content='${tts_content}'&file_path='${tts_file_path}'&tts_config='${tts_config}'"/>
                   <action application="log" data="TTS_CURL_RESPONSE tts_response=${curl_response_data}"/>
                   <action application="park"/>
               </condition>
           </extension>
           <extension name="asr_gather_digits">
               <condition field="destination_number" expression="asr_gather"/>
               <condition field="${asr_type}" expression="stop" break="on-true" >
                   <action application="kylin_grub" data="${asr_type} ${asr_ip} ${asr_port} 0 1000 ${asr_gather_silence}"/>
                   <action application="transfer" data="in_ivr_park"/>
               </condition>
               <condition field="${tts_file_exists}-${stream_tts}" expression="false-0" break="never">
                   <condition field="${tts_file_generate}" expression="true" break="never">
                       <action application="curl" data="127.0.0.1:4009/generate?tts_content='${tts_content}'&file_path='${tts_file_path}'&tts_config='${tts_config}'"/>
                       <action application="log" data="TTS_CURL_RESPONSE tts_response=${curl_response_data}"/>
                   </condition>
               </condition>
               <condition field="destination_number" expression="asr_gather">
                   <action application="set" data="tts_file_duration=0"/>
                   <action application="lua" data="wav_duration.lua ${tts_file_path} tts_file_duration 8000 16"/>
                   <action application="set" data="asr_gather_silence=${expr(${asr_gather_silence} + ${tts_file_duration})}"/>
               </condition>
               <condition field="destination_number" expression="asr_gather"/>
               <condition field="${play_type}" expression="url" break="never">
                   <action application="set" data="asr_gather_silence=60"/>
               </condition>
               <condition field="destination_number" expression="asr_gather"/>
               <condition field="${asr_type}" expression="start" break="never">
                   <action application="kylin_grub" data="${asr_type} ${asr_ip} ${asr_port} 0 1000 ${asr_gather_silence}"/>
               </condition>
               <condition field="destination_number" expression="asr_gather"/>
               <condition field="${dtmf_gather_switch}" expression="0" break="never">
                   <action application="set" data="start_play_time=${system date +%s}"/>
                   <action application="log" data="Dtmf Switch not open, file ${tts_file_path} start play at ${start_play_time}"/>
                   <action application="lua" data="asr_gather_play.lua"/>
                   <action application="park"/>
               </condition>
               <condition field="destination_number" expression="asr_gather"/>
               <condition field="${dtmf_gather_switch}" expression="1" break="never">
                   <action application="log" data="Dtmf Switch is open, file ${tts_file_path} start play at ${start_play_time}"/>
                   <action application="lua" data="gather_digits.lua ${gather_min_len} ${gather_max_len} ${gather_timeout} '${tts_file_path}' '${gather_endKey}'"/>
                   <action application="event" data="Event-Subclass=xxxxx::gather_finished,Event-Name=CUSTOM"/>
                   <action application="park"/>
               </condition>
           </extension>
           <extension name="bridge_to_agent">
               <condition field="destination_number" expression="bridge_to_agent-(.*)" require-nested="false">
                   <action application="set" data="agent_contact_num=$1" inline="true"/>
                   <action application="set" data="ignore_early_media=true"/>
                   <action application="set" data="instant_ringback=true"/>
                   <action application="set" data="ringback=${cn-ring}"/>
                   <action application="set" data="transfer_ringback=${cn-ring}"/>
                   <action application="unset" data="failure_causes"/>
                   <action application="set" data="continue_on_fail=NORMAL_TEMPORARY_FAILURE,CALL_REJECTED,USER_BUSY,NO_ANSWER,TIMEOUT,RECOVERY_ON_TIMER_EXPIRE,NETWORK_OUT_OF_ORDER,SUBSCRIBER_ABSENT"/>
                   <action application="set" data="transfer_on_fail=USER_NOT_REGISTERED,ALLOTTED_TIMEOUT,NO_ROUTE_DESTINATION proc_agent_breakout-$1"/>
                   <action application="export" data="nolocal:hangup_auto=true"/>
                   <condition field="${lin_workflow}" expression="callin">
                       <action application="export" data="transfer_after_bridge=in_ivr_park"/>
                   </condition>
               </condition>
               <condition field="${cn-ring}" expression="^Undefined$" break="never">
                   <action application="set" data="ignore_early_media=false"/>
                   <action application="set" data="instant_ringback=false"/>
               </condition>
               <condition field="${agent_contact_num}" expression="(^[1-9]\d{13}$)" break="never"/>
               <condition field="${lin_hide_number}" expression="false" break="never">
                   <action application="set" data="effective_caller_id_name=${lin_caller_number}"/>
                   <action application="set" data="effective_caller_id_number=${lin_caller_number}"/>
                   <anti-action application="set" data="effective_caller_id_name=${lin_called_display}"/>
                   <anti-action application="set" data="effective_caller_id_number=${lin_called_display}"/>
               </condition>
               <condition field="${agent_contact_num}" expression="(^[1-9]\d{13}$)" break="on-true" require-nested="false">
                   <action application="log" data="notice bridge_to_agent is voip"/>
                   <action application="bridge" data="user/$1"/>
                   <action application="log" data="notice bridge back"/>
               </condition>
               <condition field="${agent_contact_num}" expression="(^\d+$)" require-nested="false">
                   <action application="set" data="effective_caller_id_name=${lin_called_display}"/>
                   <action application="set" data="effective_caller_id_number=${lin_called_display}"/>
               </condition>
               <condition field="${agent_contact_num}" expression="^\d+$" require-nested="false">
                   <condition field="${specified_caller_to_callee}" expression="^(\d+)-to-${agent_contact_num}$" break="never">
                       <action application="set" data="effective_caller_id_name=${1}"/>
                       <action application="set" data="effective_caller_id_number=${1}"/>
                   </condition>
               </condition>
               <condition field="${agent_contact_num}" expression="(^\d+$)" require-nested="false">
                   <condition field="${lin_hide_number}" expression="false" break="never">
